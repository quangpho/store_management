@model IEnumerable<StoreManagement.Entity.Order>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dress.Drescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RenterName)
